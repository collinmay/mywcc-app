<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="edu.whatcom.mywcc">

    <application
        android:allowBackup="true"
        android:icon="@drawable/orca"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/orcaicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CalendarPage"
            android:label="@string/title_activity_calendar_page"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MapCollegePage"
            android:label="@string/title_activity_map_college_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudentProfilePage"
            android:label="@string/title_activity_student_profile_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomePageActivity"
            android:label="@string/title_activity_home_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".HomeActivity" />
        <activity
                android:name=".CurrentScheduleActivity"
                android:label="@string/title_activity_current_schedule"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>